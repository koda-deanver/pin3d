%body
  %section#main-content
    %section.wrapper.main-wrapper.row{:style => ""}
      .col-xs-12
        .page-title
          .pull-left
            / PAGE HEADING TAG - START
            %h1.title Dashboard
            / PAGE HEADING TAG - END
      .clearfix
      / MAIN CONTENT AREA STARTS
      .col-lg-12.dashboard
        %section.box
          %header.panel_header
            %h2.title.pull-left Dashboard
            / .actions.panel_actions.pull-right
            /   %a.box_toggle.fa.fa-chevron-down
            /   %a.box_setting.fa.fa-cog{"data-toggle" => "modal", :href => "#section-settings"}
            /   %a.box_close.fa.fa-times
          .content-body
            .row
              .col-xs-12
      .col-lg-12.cube-box
        %section.box
          %header.panel_header
            %h2.title.pull-left Cubes
            / .actions.panel_actions.pull-right
            /   %a.box_toggle.fa.fa-chevron-down
            /   %a.box_setting.fa.fa-cog{"data-toggle" => "modal", :href => "#section-settings"}
            /   %a.box_close.fa.fa-times
          .content-body
            .row
              .col-xs-12.no-padding
                .cube-container
                  .cube-canvas
                    .page-container
                      #dialog{title: "Basic dialog", style: "display:none;"}
                        %p Enter Cube Name here.
                        %input{type:"text"}
                      #dialog_remove{title: "Remove Cube", style: "display:none;"}
                        %p Do you want to delete this cube? All links belong to this cube will also be removed.
                      .options
                        .title Cube Name
                        %p.edit-name Edit Name
                        %p
                          %a.visit-cube{href: "/accounts/cubes/asdfasdf", target: "_blank"} Enter Cube
                        %p.remove-cube Remove
                        %p.close Close
                    / %iframe.cube-canvas{style: "width:100%; height:600px;"}
                    .cube-canvas-holder{style: "width:100%; height:600px;"}
                    .add-container
                      %button#add-a-cube
                        %i.fa.fa-plus-circle

      / MAIN CONTENT AREA ENDS