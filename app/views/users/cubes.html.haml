      / MAIN CONTENT AREA STARTS          
      .col-lg-12.cube-box{style: 'padding: 0'}
        %section{style: 'background-color: transparent;'}
          .content-body.preload-cubes.no-padding{style: 'background-color: transparent;'}
            .row
              .col-xs-12.no-padding
                .cube-container
                  .cube-canvas
                    .page-container
                      #dialog{title: "Add Cube", style: "display:none;"}
                        %p Enter Cube Name here.
                        %input{type:"text", style: "width: 100%;"}
                        %p Description
                        %textarea{:cols => "30", :name => "message", :rows => "3"}
                      #dialog_collab{title: "Collaborators", style: "display:none;"}
                        .input-group
                          %span.input-group-addon
                            %i.fa.fa-envelope
                          %span.twitter-typeahead{:style => "position: relative; display: inline-block; direction: ltr;"}
                            %input#collab.form-control
                        %input#added-collab
                      #dia_collab_confirm{title: "Confirm Email", style: "display:none;"}
                        %p
                          Your are about to add  
                          %span email
                          as a collaborator.
                      #dialog_remove{title: "Remove Cube", style: "display:none;"}
                        %p Do you want to delete this cube? All links belong to this cube will also be removed.
                      #dialog_publish{title: "Unpublish Cube", style: "display:none;"}
                        %p Do you want to unpublish this cube?
                      / Modal
                      #myModal.modal.fade{:role => "dialog"}
                        .modal-dialog
                          / Modal content
                          .modal-content
                            .modal-header
                              %button.close{"data-dismiss" => "modal", :type => "button"} ×
                              %h4.modal-title Terms & Condition
                            .modal-body
                              %p
                              .terms
                                %input{:name => "agree", :id => "agree", :type => "checkbox", :value => "1"}
                                %b I agree to the Terms and Conditions.

                            .modal-footer
                              %button.btn.btn-default.submit{:type => "button"} Submit
                              %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"} Close

                    / %iframe.cube-canvas{style: "width:100%; height:600px;"}
                    .cube-canvas-holder{style: "width:100%; height:600px;"}

                    / .add-container
                    /   %button#add-a-cube
                    /     %i.fa.fa-plus-circle
      
      %section#edit-questions-popup{:style => "display: none;max-height: 510px;min-height: 200px;background: #23394b;position: absolute;top: 15px;left: 200px;width: 500px;padding: 15px;overflow-y: auto;"}
