
%body
  %section#main-content
    %section.wrapper.main-wrapper.row{:style => "position: relative;"}
      %section#link-entry.users.row
        .col-sm-12#text-holder{style: "display: none;"}
          %h4 
            Enter Cube Name
            %i.fa.fa-times-circle
          .input-group
            %span.input-group-addon
              %i.fa.fa-cube
            %input.form-control#find-cubes{type: 'url', name: "find-cubes", placeholder: "Enter cube name to search..."}

      %section#cube-detail-entry{style: "display: none;"}
        .row
          .col-sm-12
            .form-group.has-info
              .controls
                %input#cube-name.form-control{:placeholder => "Cube Name", :type => "text"}/
          .col-sm-12
            .form-group.has-info
              .controls
                %textarea#cube-description.form-control{:placeholder => "Cube Description", :cols => "2"}
          
        .row
          .col-sm-4{style: "padding-right: 7.5px"}
            %button#btn-create-cube.btn.btn-default.btn-group-justified{:type => "button"} Create
          .col-sm-4{style: "padding-left: 7.5px; padding-right: 7.5px;"}
            %button#btn-assign-walls.btn.btn-default.btn-group-justified{:type => "button"} Assign Walls
          .col-sm-4{style: "padding-left: 7.5px"}
            %button#btn-assign-questions.btn.btn-default.btn-group-justified{:type => "button"} Assign Questions  

      %section#wall-collab-entry{style: "display: none;"}
        .row
          .col-sm-12.bottom15
            .input-group.primary
              %input#red-wall-control.form-control{"aria-describedby" => "red-wall", :placeholder => "Email", :type => "email", "data-wall" => 1}/
              %span#red-wall.input-group-addon
                %span.arrow

          .col-sm-12.bottom15
            .input-group.primary
              %input#green-wall-control.form-control{"aria-describedby" => "green-wall", :placeholder => "Email", :type => "email", "data-wall" => 5}/
              %span#green-wall.input-group-addon
                %span.arrow

          .col-sm-12.bottom15
            .input-group.primary
              %input#blue-wall-control.form-control{"aria-describedby" => "blue-wall", :placeholder => "Email", :type => "email", "data-wall" => 3}/
              %span#blue-wall.input-group-addon
                %span.arrow

          .col-sm-12.bottom15
            .input-group.primary
              %input#yellow-wall-control.form-control{"aria-describedby" => "yellow-wall", :placeholder => "Email", :type => "email", "data-wall" => 4}/
              %span#yellow-wall.input-group-addon
                %span.arrow

          .col-sm-12.bottom15
            .input-group.primary
              %input#sky-blue-wall-control.form-control{"aria-describedby" => "sky-blue-wall", :placeholder => "Email", :type => "email", "data-wall" => 2}/
              %span#sky-blue-wall.input-group-addon
                %span.arrow

          .col-sm-12.bottom15
            .input-group.primary
              %input#magenta-wall-control.form-control{"aria-describedby" => "magenta-wall", :placeholder => "Email", :type => "email", "data-wall" => 0}/
              %span#magenta-wall.input-group-addon
                %span.arrow
          .col-sm-12
            %button#close-asign.btn.btn-default.btn-group-justified{:type => "button"} Close

      %section#assign-question-entry{style: "display: none;"}
        .row
          #category-accordion.panel-group.questions-popup{:style => "padding: 0 15px;"}

      #cube-collab-indicator{style: "display: none;"}
        .indicator-holder
          .title 
            / %i.fa.fa-header.text-success 
            %span Cube Name
          .description
          %p.uilabels.text-md
            %span.label.label-info
              %i.fa.fa-info
              Collaborator
          %i.fa.fa-caret-right

      .col-lg-12.bg-floater
