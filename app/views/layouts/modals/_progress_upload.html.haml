    .modal-content{:style => "min-width: 100vh;"}
      .modal-body{style: "padding: 5px 0 0 0;"}
        .knobcharts-area
          .col-sm-4.col-sm-offset-4.bottom5
            %div{:style => "display: block; height: 60px; width: 60px; margin: auto;"}
              %input.knob{"data-displayinput" => "true", "data-fgcolor" => "#3F51B5", "data-width" => "100%", 'data-readOnly'=> "true", :style => "display: none; width: 0px; visibility: hidden;", :value => "0"}/


