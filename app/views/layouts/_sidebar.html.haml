    -if action_name == 'dashboard'
      %ul.wraplist
        / %li.menusection Main
        %li.home{class: "#{open_link('users', 'dashboard')}"}
          = link_to dashboard_path do
            %i.fa.fa-dashboard
            %span.title Dashboard
        %li.cubes{class: "#{open_link('users', 'cubes')}"}
          = link_to cubes_users_path do
            %i.fa.fa-cube
            %span.title Cubes
        %li.profile{class: "#{open_link('users', 'profile')} #{open_link('users', 'edit')}"}
          = link_to profile_users_path do
            %i.fa.fa-user
            %span.title Profile
    -elsif action_name == 'cubes'
      %ul.wraplist.top-align.text-right
        %li.create
          %a#new-project
            %span.title CREATE NEW PROJECT
        %li.find
          = link_to cubes_users_path do
            %span.title SEARCH PUBLISHED CUBES

      %ul.wraplist.bottom-align.options.text-right
        %li
          %a.title.text-uppercase{style: 'color: #ec028c !important;'} Cube Name
        %li
          %a.visit-cube{href: "/accounts/cubes/asdfasdf"} ENTER CUBE
        %li
          %a.make-favorite FAVORITE
        %li
          %a.edit-name  EDIT CUBE
        %li
          %a.edit-questions  CUBE QUESTIONS  
        %li
          %a.remove-cube REMOVE
        %li
          %a.share-cube PUBLISH
        %li
          %a.close-menu  CLOSE
    - else
      %section.box.inverted
        %header.panel_header.ui-sortable-handle
          %h2.cube-name.title.title.pull-left
            %i.fa.fa-spinner.rotating
      %ul.nav.nav-tabs.transparent
        %li.active
          %button{"data-toggle" => "tab", :href => "#home-2"}
            / %i.fa.fa-link
            Links
        %li
          %button{"data-toggle" => "tab", :href => "#profile-2"}
            / %i.fa.fa-user
            Collaborators

      .tab-content.transparent
