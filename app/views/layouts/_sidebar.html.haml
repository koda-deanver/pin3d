.page-sidebar.fixedscroll{:class => ('grid-sidebar' if action_name == 'cubes')} 
  / MAIN MENU - START
  #main-menu-wrapper.page-sidebar-wrapper
    / USER INFO - START
    / .profile-info.row
    /   .profile-image.col-xs-4
    /     %a{:href => "ui-profile.html"}
    /       %img.img-responsive.img-circle{:alt => "", :src => "/assets/profile.jpg"}
    /     .profile-details.col-xs-8
    /       %h3
    /         %a{:href => "ui-profile.html"} Shane Taylor
    /         / Available statuses: online, idle, busy, away and offline
    /         %span.profile-status.online
